{
  "name": "Mopidy-MusicBox-Webclient",
  "version": "2.1.1",
  "description": "Mopidy MusicBox web extension",
  "main": "gui.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "build": "npm run browserify && npm run uglify",
    "eslint": "eslint mopidy_musicbox_webclient/static/js/**/**.js tests/**/test_*.js",
    "csslint": "csslint mopidy_musicbox_webclient/static/css/**.css",
    "tidy": "node tidy.js",
    "browserify":"browserify --standalone Webclient mopidy_musicbox_webclient/src/webclient.js -o mopidy_musicbox_webclient/static/js/webclient.js",
    "uglify": "uglifyjs mopidy_musicbox_webclient/static/js/webclient.js -o mopidy_musicbox_webclient/static/js/webclient.min.js --source-map mopidy_musicbox_webclient/static/js/webclient.min.js.map --source-map-root http://127.0.0.1:6680/mopidy/musicbox_webclient -p 1 -c -m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pimusicbox/mopidy-musicbox-webclient.git"
  },
  "author": "Wouter van Wijk",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pimusicbox/mopidy-musicbox-webclient/issues"
  },
  "devDependencies": {
    "babelify": "~7.2.0",
    "bane": "~1.1.2",
    "browserify": "~13.0.0",
    "browserify-istanbul": "~2.0.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.2.0",
    "chai-jquery": "~2.0.0",
    "chai-string": "~1.2.0",
    "coveralls": "~2.11.8",
    "csslint": "~0.10.0",
    "eslint": "~2.3.0",
    "eslint-config-standard": "~5.1.0",
    "eslint-plugin-promise": "~1.1.0",
    "eslint-plugin-standard": "~1.3.2",
    "faye-websocket": "~0.11.0",
    "install": "~0.5.6",
    "isparta": "~4.0.0",
    "karma": "~0.13.22",
    "karma-browserify": "~5.0.2",
    "karma-cli": "~0.1.2",
    "karma-coverage": "~0.5.5",
    "karma-mocha": "~0.2.2",
    "karma-phantomjs-launcher": "~1.0.0",
    "mocha": "~2.4.5",
    "phantomjs-prebuilt": "~2.1.5",
    "sinon": "~1.17.3",
    "tidy-html5": "~0.1.1",
    "uglify-js": "~2.6.2",
    "watchify": "~3.7.0",
    "when": "~3.7.7"
  },
  "homepage": "https://github.com/pimusicbox/mopidy-musicbox-webclient#readme"
}
